<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1" xmlns:ns4="null">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>Anagram Modular</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <name>Anagram Modular</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>src-api</label>
                    <type>java</type>
                    <location>src-api</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Anagram Modular</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
                <action name="debug.single">
                    <target>-do-test</target>
                    <context>
                        <property>include</property>
                        <folder>src-test</folder>
                        <format>relative-path-noext</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/api/classes</location>
                <build-target>build</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src-api</label>
                        <location>src-api</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src-api</package-root>
                <built-to>build/api/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src-test</package-root>
                <classpath mode="compile">src-new-api:../libs/dist/junit-4.4.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
